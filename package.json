{
  "name": "jsdoc-example-tester",
  "version": "0.0.3",
  "main": "./lib/app.js",
  "types": "./lib/app.d.ts",
  "bin": {
    "tsdoc-testify": "./lib/cli.js"
  },
  "license": "Apache-2.0",
  "repository": "akito0107/tsdoc-testify",
  "author": {
    "name": "Akito Ito",
    "email": "akito0107@gmail.com"
  },
  "scripts": {
    "test": "vitest src/__tests__",
    "lint": "eslint ./src/**/*.ts",
    "fmt": "eslint --fix ./src/**/*/ts",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./lib/*",
    "examplesdoc": "typedoc --out examplesdoc examples",
    "try:filepath": "npm run build && node ./lib/cli.js --filepath ./examples/sample.ts",
    "try:fileMatch": "npm run build && node ./lib/cli.js --fileMatch ./examples/sample"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^22.15.2",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "catacli": "^0.1.3",
    "glob": "^11.0.2",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "tsutils": "^3.21.0",
    "typescript": "^5.8.3"
  }
}
