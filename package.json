{
  "name": "jsdoc-example-tester",
  "version": "0.1.0",
  "main": "./lib/app.js",
  "types": "./lib/app.d.ts",
  "bin": {
    "jsdoc-example-tester": "./lib/cli.js"
  },
  "license": "Apache-2.0",
  "repository": "SacDeNoeuds/jsdoc-example-tester",
  "author": {
    "name": "SacDeNoeuds",
    "email": "sacdenoeuds.dev@gmail.com"
  },
  "scripts": {
    "test": "vitest src/__tests__",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./lib/*",
    "examplesdoc": "typedoc --out examplesdoc examples",
    "try:filepath": "npm run build && node ./lib/cli.js --filepath ./examples/sample.ts",
    "try:fileMatch": "npm run build && node ./lib/cli.js --fileMatch ./examples/sample"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^22.15.2",
    "@types/prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "catacli": "^0.1.3",
    "glob": "^11.0.2",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "tsutils": "^3.21.0",
    "typescript": "^5.8.3"
  }
}
